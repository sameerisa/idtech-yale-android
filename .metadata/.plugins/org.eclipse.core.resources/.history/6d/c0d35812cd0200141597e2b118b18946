package com.example.astroids;

import android.graphics.Bitmap;
import android.graphics.Canvas;

public class Spaceship {
	private Bitmap bitmap;
	private int x;
	private int y;
	private int xVelocity;

	public Spaceship(Bitmap bitmap, int x, int y) {
		this.bitmap = bitmap;
		this.x = x;
		this.y = y;
	}

}
